{
  "name": "forte-dev",
  "author": "Bartek Kus <bartekus@gmail.com> (https://bartekus.com/)",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.15.3"
  },
  "description": "Full-stack baseline for startup development",
  "homepage": "https://github.com/forte-dev/nx-react-express-hasura-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/forte-dev/nx-react-express-hasura-starter.git"
  },
  "bugs": {
    "url": "https://github.com/forte-dev/nx-react-express-hasura-starter/issues"
  },
  "scripts": {
    "nx": "nx",
    "init": "npm run generate:rsa-keys && npm run generate:hasura-env && npm run generate:server-env",
    "db:build": "cd hasura && docker-compose up -d",
    "db:rebuild": "cd hasura && docker-compose up --build",
    "db:console": "cd hasura && hasura console",
    "generate:hasura-env": "node utils/make-hasura-env",
    "generate:server-env": "node utils/make-server-env",
    "generate:rsa-keys": "node utils/rsa-keys-gen",
    "dev": "nx run-many --parallel --target=serve --projects=client,api",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@material-ui/core": "next",
    "@material-ui/icons": "next",
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "express": "4.17.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@nrwl/cli": "11.0.20",
    "@nrwl/cypress": "11.0.20",
    "@nrwl/eslint-plugin-nx": "11.0.20",
    "@nrwl/express": "11.0.20",
    "@nrwl/jest": "11.0.20",
    "@nrwl/node": "11.0.20",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "11.0.20",
    "@nrwl/tao": "11.0.20",
    "@nrwl/web": "11.0.20",
    "@nrwl/workspace": "11.0.20",
    "@testing-library/react": "11.1.2",
    "@types/express": "4.17.0",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "26.2.2",
    "cypress": "^5.5.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^4.3.6",
    "jest": "26.2.2",
    "lint-staged": "^10.5.3",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx}": [
      "npm run format --uncommitted"
    ]
  }
}
